prototype(Plyr:Notification) < prototype(Fusion:Tag) {
	@if.render = ${node.context.inBackend && this.content}

	key = false

	attributes.style = "background:#d9534f;color:#fff;border:1px solid #d43f3a;color:#fff;padding:5% 12px;margin:30px auto;text-align:center;text-decoration:none;outline:none;font-size:20px;"
	content = ${this.key ? Translation.translate('backendAlert.' + this.key, null, [], null, 'Jonnitto.Plyr') : false}
}
